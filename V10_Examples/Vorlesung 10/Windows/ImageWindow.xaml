<Window x:Class="Vorlesung_10.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Images" Height="600" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Image Grid.Row="0" Source="../Bilder/OST-Logo.jpg" Stretch="Uniform" />
        <Image Grid.Row="1" Source="../Bilder/OST-Logo.jpg" Stretch="Fill" />
        <Image Grid.Row="2" Source="../Bilder/OST-Logo.jpg" Stretch="UniformToFill" />
        <Image Grid.Row="3" Source="../Bilder/OST-Logo.jpg" Stretch="None" />
    </Grid>
</Window>
